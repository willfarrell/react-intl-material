    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.13.1",
    "eslint-config-react": "^1.1.7",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.5.0",
